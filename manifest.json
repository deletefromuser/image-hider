{
    "manifest_version": 2,
    "name": "Image Hider",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "tabs", "http://*/*", "https://*/*"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "all_frames": true,
        "matches": ["*://*/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "options_page": "options.html"
  }