{
    "manifest_version": 3,
    "name": "Image Hider",
    "version": "1.0",
    "host_permissions": [
        "http://*/*",
        "https://*/*"
      ],
    "permissions": ["activeTab", "storage", "scripting", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html"
  }